{
  "name": "project_device",
  "comment": "项目设备",
  "joins": [
    {
      "table": "project",
      "local_field": "project_id",
      "foreign_field": "id",
      "field": "name",
      "as": "project"
    },
    {
      "table": "device",
      "local_field": "device_id",
      "foreign_field": "id",
      "field": "name",
      "as": "device"
    }
  ],
  "fields": [
    {
      "name": "project_id",
      "comment": "项目ID",
      "type": "string",
      "length": 50,
      "primary": true
    },
    {
      "name": "device_id",
      "comment": "设备ID",
      "type": "string",
      "length": 50,
      "primary": true
    },
    {
      "name": "name",
      "comment": "别名",
      "type": "string",
      "length": 128
    },
    {
      "name": "created",
      "comment": "创建时间",
      "type": "datetime",
      "created": true
    }
  ]
}