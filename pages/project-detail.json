{
  "title": "项目详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "page": "iot/project-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('table/project/delete/'+data.id).subscribe(res=>{this.navigate('/page/iot/project')})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "description",
      "label": "说明"
    },
    {
      "key": "disabled",
      "label": "禁用"
    }
  ],
  "load_api": "table/project/detail/:id",
  "tabs": [
    {
      "title": "项目空间",
      "page": "iot/space",
      "params_func": "return {project_id: params.id}"
    },
    {
      "title": "项目设备",
      "page": "iot/project-device",
      "params_func": "return {project_id: params.id}"
    },
    {
      "title": "项目报警",
      "page": "iot/alarm",
      "params_func": "return {project_id: params.id}"
    },
    {
      "title": "项目用户",
      "page": "iot/project-user",
      "params_func": "return {project_id: params.id}"
    },
    {
      "title": "项目插件",
      "page": "iot/project-plugin",
      "params_func": "return {project_id: params.id}"
    }
  ]
}