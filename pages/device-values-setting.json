{
  "title": "设置属性",
  "template": "form",
  "fields": [
    {
      "key": "key",
      "type": "select",
      "label": "属性",
      "options": []
    },{
      "key": "value",
      "type": "number",
      "label": "值"
    }
  ],
  "mount": "this.load_device()",
  "submit": "this.request.post('iot/device/'+this.params.id+'/write', {[data.key]:data.value}).subscribe(res=>{if(res.error)return; })",
  "methods": {
    "load_device": "this.request.get('table/device/detail/'+this.params.id).subscribe(res=>{if(res.error)return; this.load_model(res.data.product_id)})",
    "load_model": ["pid","this.request.get('iot/product/'+pid+'/model').subscribe(res=>{if(res.error)return; this.render_properties(res.data.properties) })"],
    "render_properties": ["properties", "console.log(properties); this.content.fields[0].options=[]; properties.map(p=>this.render_points(p.points) )"],
    "render_points": ["points", "console.log(points); points.map(p=>{this.content.fields[0].options.push({value:p.name,label:p.label}) }); console.log(this.content.fields[0].options);"]
  }
}