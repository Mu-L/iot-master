{
  "title": "项目设备",
  "template": "table",
  "operators": [
    {
      "icon": "check",
      "label": "选择",
      "action": {
        "type": "script",
        "script": "this.modelRef.close(data)"
      }
    }
  ],
  "columns": [
    {
      "key": "device",
      "label": "设备"
    },
    {
      "key": "name",
      "label": "名称"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "search_api": "table/project-device/search",
  "mount": "if(this.params.project_id)this.filter.project_id=this.params.project_id;"
}